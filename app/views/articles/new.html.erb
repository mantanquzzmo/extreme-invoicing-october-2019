<h2>Create article</h2>
<%= form_with model: @article do |form|%>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :unit %>
  <%= form.text_field :unit %>
  <%= form.label :unit_price %>
  <%= form.text_field :unit_price %>
  <%= form.label :taxrate %>
  <%= form.text_field :taxrate %>
  <%= form.submit "Add article" %>
<% end %> 